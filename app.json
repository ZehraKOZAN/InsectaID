{
  "expo": {
    "name": "InsectaID",
    "slug": "insectaid",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "insectaid",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F2E1A"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.insectaid"
    },
    "android": {
      "package": "com.insectaid",
      "adaptiveIcon": {
        "backgroundColor": "#0F2E1A",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://replit.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow InsectaID to access your camera to identify insects."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow InsectaID to access your photos to identify insects."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
